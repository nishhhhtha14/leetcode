class Solution {
public:
    int removeElement(vector<int>& nums, int val) {
       int n=nums.size();
        vector<int> v(n);
        int c=0;
        for(int i=0;i<n;i++)
        {
            if(nums[i]!=val)
                v[c++]=nums[i];
        }  
            nums=v;
            return c;
        
    }
};
